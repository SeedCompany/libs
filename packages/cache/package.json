{
  "name": "@seedcompany/cache",
  "version": "3.1.0",
  "description": "NestJS library wrapper for external caching",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@nestjs/common": "^10 || ^11",
    "@nestjs/core": "^10 || ^11",
    "@seedcompany/common": ">0.3 <1",
    "@types/luxon": "^3.2.0",
    "luxon": "^3.3.0",
    "type-fest": "^4.26.1"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "^3",
    "@nestjs/common": "^9 || ^10 || ^11",
    "@nestjs/core": "^9 || ^10 || ^11",
    "ioredis": "^5",
    "keyv": "^5",
    "lru-cache": "^9 || ^10 || ^11",
    "reflect-metadata": "^0.1.12 || ^0.2.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/cache-manager": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "keyv": {
      "optional": true
    },
    "lru-cache": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc",
    "test": "node --experimental-vm-modules ../../node_modules/.bin/vitest"
  },
  "devDependencies": {
    "@apollo/utils.keyvaluecache": "^2.1.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/testing": "^11.0.11",
    "cache-manager": "^6.4.1",
    "ioredis": "^5.3.1",
    "keyv": "^5.3.1",
    "lru-cache": "^11.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcompany/libs.git",
    "directory": "packages/cache"
  },
  "bugs": {
    "url": "https://github.com/seedcompany/libs/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./stores/*": {
      "types": "./dist/stores/*.d.ts",
      "default": "./dist/stores/*.js"
    }
  }
}
