{
  "name": "@seedcompany/common",
  "version": "0.4.0",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "luxon": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "luxon": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/luxon": "^3.2.0",
    "luxon": "^3.3.0"
  },
  "scripts": {
    "build": "tsup src/index.ts src/temporal/luxon/index.ts --external=\"@seedcompany/common\" --format cjs,esm --dts --clean",
    "typecheck": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcompany/libs.git",
    "directory": "packages/common"
  },
  "bugs": {
    "url": "https://github.com/seedcompany/libs/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./temporal/luxon": {
      "import": "./dist/temporal/luxon/index.js",
      "require": "./dist/temporal/luxon/index.cjs",
      "types": "./dist/temporal/luxon/index.d.ts"
    }
  }
}
